}
Push SelectXSmallPressed {
	SizeSelected = 5;
	SetSize();
}
// #EndRegion


Push SelectSlidersSelected {
	SetType();
	SetSize();
	SetShape();
	SetFeature();
}

Release SelectSlidersSelected {
	UnselectAllTypeButtons();
	ClearAllSizePages();
	UnselectAllSizeButtons();
}


