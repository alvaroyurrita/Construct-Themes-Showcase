	SliderFaderTitle = "Sliders<br/>Fader";
	SliderGaugeTitle = "Sliders<br/>Gauge";
	SliderRangeTitle = "Sliders<br/>Range";

	ToolTipTicksTitle = "Tool Tip<br/>+ Ticks";
	ToolTipTitle = "Tool Tip";
	FeatureSelected = 3;
	SimpleTitle = "Simple";

	CircleTitle = "Circle";
	OvalTitle = "Oval";
	ShapeSelected = 3;
	RoundRectangleTitle = "Round<br/>Rectangle";
	RectangleTitle = "Rectangle";

	LargeTitle = "Large";
