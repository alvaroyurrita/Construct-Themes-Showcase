// #Region Size
Function UnselectAllSizeButtons() {
	SelectXLargeSelected = 0;
	SelectLargeSelected = 0;
	SelectRegularSelected = 0;
	SelectSmallSelected = 0;
	SelectXSmallSelected = 0;
}
Function ClearAllSizePages() {
	XLargePageView = 0;
	LargePageView = 0;
	RegularPageView = 0;
	SmallPageView = 0;
	XSmallPageView = 0;
}
Function SetSizePage() {
	CSwitch (SizeSelected) {
		Case (1): {
			LargePageView = 1;
			Break;
		}
		Case (2): {
			RegularPageView = 1;
			Break;
		}
		Case (3): {
			SmallPageView = 1;
			Break;
		}
		Case (4): {
			XLargePageView = 1;
			Break;
		}
		Case (5): {
			XSmallPageView = 1;
			Break;
		}
	}
}
Function SetSizeButtons() {
	CSwitch (SizeSelected) {
		Case (1): {
			SelectLargeSelected = 1;
			Break;
		}
		Case (2): {
			SelectRegularSelected = 1;
			Break;
		}
		Case (3): {
			SelectSmallSelected = 1;
			Break;
		}
		Case (4): {
			SelectXlargeSelected = 1;
			Break;
		}
		Case (5): {
			SelectXSmallSelected = 1;
			Break;
		}
	}
}
Function SetSize() {
	UnselectAllSizeButtons();
	SetSizeButtons();
	ClearAllSizePages();
	SetSizePage();
	SelectWidget();
}
Push SelectLargePressed {
	SizeSelected = 1;
	SetSize();
}
Push SelectRegularPressed {
	SizeSelected = 2;
	SetSize();
}
Push SelectSmallPressed {
	SizeSelected = 3;
