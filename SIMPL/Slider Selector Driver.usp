		Case (3): {
			SelectRoundRectangleSelected = 1;
			Break;
		}
		Case (4): {
			SelectRectangleSelected = 1;
			Break;
		}
	}
}
Function SetShape() {
	UnselectAllShapeButtons();
	SetShapeButtons();
	SelectWidget();
}
