Integer SliderTypeSelected;
Integer ShapeSelected;
Integer FeatureSelected;
Integer SizeSelected;

Function SelectWidget() {
	Integer selectedWidget, i;

	selectedWidget = (SizeSelected - 1) * 12 + (ShapeSelected - 1) * 3 + FeatureSelected;
	For (i = 1 to 60) {
		If (i = selectedWidget) {
			Widgets[i] = 1;
		}
		Else {
			Widgets[i] = 0;
		}
	}
}

// #Region Enable Buttons

Function EnableAllTypeButtons() {
	SelectSlidersAdvancedEnabled = 1;
	SelectSlidersFaderEnabled = 1;
	SelectSlidersGaugeEnabled = 1;
	SelectSlidersRangeEnabled = 1;
}

Function EnableAllShapeButtons() {
	SelectRoundRectangleEnabled = 1;
	SelectCircleEnabled = 1;
	SelectRectangleEnabled = 1;
	SelectOvalEnabled = 1;
}

Function EnableAllFeatureButtons() {
	SelectSimpleEnabled = 1;
	SelectToolTipEnabled = 1;
	SelectToolTipTicksEnabled = 1;
}

Function EnableAllSizeButtons() {
	SelectXLargeEnabled = 1;
	SelectLargeEnabled = 1;
	SelectRegularEnabled = 1;
	SelectSmallEnabled = 1;
	SelectXSmallEnabled = 1;
}
// #EndRegion

// #Region Type
Function UnselectAllTypeButtons() {
	SelectSlidersAdvancedSelected = 0;
	SelectSlidersFaderSelected = 0;
	SelectSlidersGaugeSelected = 0;
	SelectSlidersRangeSelected = 0;
}
Function SetTypeButtons() {
	CSwitch (SliderTypeSelected) {
		Case (1): {
			SelectSlidersAdvancedSelected = 1;
			EnableAllShapeButtons();
			EnableAllSizeButtons();
			EnableAllFeatureButtons();
			ToolTipTicksTitle = "Tool Tip<br/>+ Ticks";
			Break;
		}
		Case (2): {
			SelectSlidersFaderSelected = 1;
			EnableAllShapeButtons();
			EnableAllSizeButtons();
			EnableAllFeatureButtons();
			ToolTipTicksTitle = "Tool Tip<br/>+ Ticks";
			Break;
		}
		Case (3): {
			SelectSlidersGaugeSelected = 1;
			ToolTipTicksTitle = "Ticks";

			SelectRoundRectangleEnabled = 0;
			SelectCircleEnabled = 0;
			SelectRectangleEnabled = 0;
			SelectOvalEnabled = 0;

			SelectXLargeEnabled = 0;
			SelectLargeEnabled = 0;
			SelectRegularEnabled = 0;
			SelectSmallEnabled = 0;
			SelectXSmallEnabled = 0;

			SelectToolTipEnabled = 0;
			Break;
		}
		Case (4): {
			SelectSlidersRangeSelected = 1;
			EnableAllShapeButtons();
			EnableAllSizeButtons();
			EnableAllFeatureButtons();
			ToolTipTicksTitle = "Tool Tip<br/>+ Ticks";
			Break;
		}
	}
}
Function SetType() {
	UnselectAllTypeButtons();
	SetTypeButtons();
	SelectWidget();
}
Push SelectSlidersAdvancedPressed {
	SliderTypeSelected = 1;
	SetType();
}
Push SelectSlidersFaderPressed {
	SliderTypeSelected = 2;
	SetType();
}
Push SelectSlidersGaugePressed {
	SliderTypeSelected = 3;
	SetType();
}
Push SelectSlidersRangePressed {
	SliderTypeSelected = 4;
	SetType();
}
// #EndRegion

// #Region Feature
Function UnselectAllFeatureButtons() {
	SelectSimpleSelected = 0;
	SelectToolTipSelected = 0;
	SelectToolTipTicksSelected = 0;
}
Function SetFeatureButtons() {
	CSwitch (FeatureSelected) {
		Case (1): {
			SelectToolTipTicksSelected = 1;
			EnableAllTypeButtons();
			Break;
		}
		Case (2): {
			SelectToolTipSelected = 1;
			SelectSlidersGaugeEnabled = 0;
			Break;
		}
		Case (3): {
			SelectSimpleSelected = 1;
			EnableAllTypeButtons();
			Break;
		}
	}
}
Function SetFeature() {
	UnselectAllFeatureButtons();
	SetFeatureButtons();
	SelectWidget();
}
Push SelectSimplePressed {
	FeatureSelected = 1;
	SetFeature();
}
Push SelectToolTipPressed {
	FeatureSelected = 2;
	SetFeature();
}
Push SelectToolTipTicksPressed {
	FeatureSelected = 3;
	SetFeature();
}
// #EndRegion

